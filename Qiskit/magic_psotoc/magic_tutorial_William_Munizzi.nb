Cell[BoxData[
 RowBox[{
  RowBox[{"DickeState", "[", 
   RowBox[{"n_", ",", "k_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"base", "=", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", "1", "}"}], ",", 
           RowBox[{"{", "0", "}"}]}], "}"}], ",", "n"}], "]"}]}], ",", 
      RowBox[{"positions", " ", "=", " ", 
       RowBox[{"Subsets", "[", 
        RowBox[{
         RowBox[{"Range", "[", "n", "]"}], ",", 
         RowBox[{"{", "k", "}"}]}], "]"}]}], ",", "replacements", ",", 
      "states", ",", "final"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"replacements", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"positions", "[", 
          RowBox[{"[", 
           RowBox[{"i", ",", "j"}], "]"}], "]"}], "->", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", "0", "}"}], ",", 
           RowBox[{"{", "1", "}"}]}], "}"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Length", "[", "positions", "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "k"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"states", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"ReplacePart", "[", 
         RowBox[{"base", ",", 
          RowBox[{"replacements", "[", 
           RowBox[{"[", "l", "]"}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"l", ",", 
          RowBox[{"Length", "[", "replacements", "]"}]}], "}"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"final", " ", "=", " ", 
      RowBox[{"Total", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"KroneckerProduct", "@@", 
          RowBox[{"states", "[", 
           RowBox[{"[", "m", "]"}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"m", ",", 
           RowBox[{"Length", "[", "states", "]"}]}], "}"}]}], "]"}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"1", "/", 
       RowBox[{"(", 
        RowBox[{"Sqrt", "[", 
         RowBox[{"Binomial", "[", 
          RowBox[{"n", ",", "k"}], "]"}], "]"}], ")"}]}], "*", "final"}]}]}], 
   "]"}], " "}]], "Input",
